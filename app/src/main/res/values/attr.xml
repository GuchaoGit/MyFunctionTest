<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SideBarView">
        <attr name="sidebar_nor_background" format="color"/>
        <attr name="sidebar_press_background" format="color"/>
        <attr name="sidebar_text_size" format="dimension"/>
        <attr name="sidebar_text_color_nor" format="color"/>
        <attr name="sidebar_text_color_press" format="color"/>
    </declare-styleable>
    <declare-styleable name="FoldView">
        <attr name="desc_text_color" format="color"/>
        <attr name="desc_text_size" format="dimension"/>
        <attr name="is_text_visible" format="boolean"/>
        <attr name="arrow_down_drawable" format="dimension"/>
        <attr name="arrow_up_drawable" format="dimension"/>
    </declare-styleable>

</resources>