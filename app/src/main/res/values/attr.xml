<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="SideBarView">
        <attr name="sidebar_nor_background" format="color"/>
        <attr name="sidebar_press_background" format="color"/>
        <attr name="sidebar_text_size" format="dimension"/>
        <attr name="sidebar_text_color_nor" format="color"/>
        <attr name="sidebar_text_color_press" format="color"/>
    </declare-styleable>
    <declare-styleable name="FoldView">
        <attr name="desc_text_color" format="color"/>
        <attr name="desc_text_size" format="dimension"/>
        <attr name="is_text_visible" format="boolean"/>
        <attr name="arrow_down_drawable" format="dimension"/>
        <attr name="arrow_up_drawable" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ViewNoData">
        <attr name="hintIcon" format="reference" />
        <attr name="hint1" format="reference|string" />
        <attr name="hint2" format="reference|string" />
        <attr name="mulLineHint" format="boolean" />
        <attr name="topIconMargin" format="dimension" />
    </declare-styleable>
    <attr name="title" format="reference|string" />
    <attr name="titleTextColor" format="color" />
    <declare-styleable name="ViewRichText">
        <attr name="title" />
        <attr name="content" />
        <attr name="titleTextColor" />
        <attr name="contentTextColor" format="color" />
    </declare-styleable>
    <declare-styleable name="ToolBar">
        <attr name="leftType">
            <enum name="none" value="0" />
            <enum name="finish" value="1" />
            <enum name="function" value="2" />
        </attr>
        <attr name="leftIcon" format="reference" />
        <attr name="title" />
        <attr name="titleTextColor" />
        <attr name="titleTextSize" format="dimension" />
        <attr name="rightType">
            <enum name="none" value="0" />
            <enum name="icon" value="1" />
            <enum name="string" value="2" />
        </attr>
        <attr name="rightIcon" format="reference" />
        <attr name="rightString" format="reference|string" />
        <attr name="isTransBg" format="boolean" />
    </declare-styleable>
    <declare-styleable name="WheelView">
        <attr name="pickerview_gravity" format="integer" />
        <attr name="pickerview_textColorOut" format="reference" />
        <attr name="pickerview_textColorCenter" format="reference" />
        <attr name="pickerview_dividerColor" format="reference" />
        <attr name="pickerview_textSize" format="dimension" />
        <attr name="pickerview_lineSpacingMultiplier" format="float" />
    </declare-styleable>
</resources>